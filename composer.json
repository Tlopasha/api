{
    "name": "antaresproject/module-api",
    "description": "Antares Api Module. Design your application without focus to restfull api driven development. Just create standard Antares application, API layer will be transformed without any interference.",
    "type": "antaresproject-module",
    "version": "0.9.2.1",
    "homepage": "https://github.com/antaresproject/api",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Antares Team",
            "email": "contact@antaresproject.io"
        }
    ],
    "require": {
        "antaresproject/component-installer-plugin": "0.9.2.x-dev",
        "antaresproject/core": "0.9.2.x-dev",
        "antaresproject/component-users": "0.9.2.x-dev",    
        "antaresproject/component-logger": "0.9.2.x-dev",        
        "laravel/framework": "5.4.*",        
        "dingo/api": "~1.0",
        "barryvdh/laravel-cors": "v0.8.*",        
        "php": ">=7.0.1"
    },
    "autoload": {
        "psr-4": {
            "Antares\\Modules\\Api\\": "src/"
        }
    },
    "extra": {
        "friendly-name": "API"
    }
}
